{
  "name": "@tholden11/n8n-nodes-zoho-mail",
  "version": "0.1.6",
  "description": "Zoho Mail (messages, attachments, accounts) node for n8n",
  "license": "MIT",
  "author": "Tom Holden tholden11@hotmail.com",
  "keywords": [
    "n8n-community-node-package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tholden101/n8n-nodes-zoho-mail.git"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      {
        "name": "ZohoMailOAuth2Api",
        "type": "credentials",
        "files": [
          "dist/credentials/ZohoMailOAuth2Api.credentials.js"
        ]
      }
    ],
    "nodes": [
      {
        "name": "ZohoMail",
        "type": "node",
        "files": [
          "dist/nodes/ZohoMail/ZohoMail.node.js"
        ]
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run lint",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "echo \"No tests yet\"",
    "pack": "npm pack"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "n8n-core": "^1.9.3",
    "n8n-workflow": "^1.9.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {}
}
